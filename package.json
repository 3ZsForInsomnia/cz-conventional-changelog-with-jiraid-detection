{
  "name": "@3zs4insomnia/cz-conventional-changelog-with-jiraid-detection",
  "version": "1.0.5",
  "description": "Commitizen adapter following the conventional-changelog format with an extra step to insert Jira ID. Forked to support detecting lower case Jira IDs",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha *.test.js",
    "format": "prettier --write *.js"
  },
  "homepage": "https://github.com/3ZsForInsomnia/cz-conventional-changelog-with-jiraid-detection",
  "repository": {
    "type": "git",
    "url": "https://github.com/3ZsForInsomnia/cz-conventional-changelog-with-jiraid-detection"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "index.js",
    "engine.js"
  ],
  "author": "Zachary Levine <zach@zjlevine.dev>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "commitizen": "^4.0.3",
    "conventional-commit-types": "^3.0.0",
    "git-branch": "^2.0.1",
    "lodash.map": "^4.5.1",
    "longest": "^2.0.1",
    "word-wrap": "^1.0.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.6",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/chai": "4.3.3",
    "@types/mocha": "9.1.1",
    "chai": "4.3.6",
    "cosmiconfig": "5.2.1",
    "mocha": "9.2.2",
    "mock-require": "3.0.3",
    "prettier": "1.19.1",
    "semantic-release": "19.0.5",
    "semver": "6.3.0"
  },
  "optionalDependencies": {
    "@commitlint/load": ">6.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./index.js"
    }
  }
}
